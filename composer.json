{
    "name": "masterkey/repository",
    "description": "Repository Pattern for Laravel 5",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Matheus Lopes Santos",
            "email": "fale_com_lopez@hotmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "ext-pdo": "*",
        "ext-json": "*",
        "php": ">=7.2.5",
        "illuminate/filesystem": "^7.0",
        "illuminate/support": "^7.0",
        "illuminate/cache": "^7.0",
        "illuminate/container": "^7.0",
        "illuminate/config": "^7.0",
        "illuminate/database": "^7.0",
        "illuminate/events": "^7.0",
        "illuminate/pagination": "^7.0",
        "illuminate/http": "^7.0",
        "illuminate/validation": "^7.0",
        "league/fractal": "^0.18.0"
    },
    "autoload": {
        "psr-4": {
            "Masterkey\\Repository\\": "src/",
            "Masterkey\\Tests\\": "tests/"
        },
        "files": [
            "src/Exceptions/RepositoryException.php"
        ]
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Masterkey\\Repository\\Providers\\RepositoryServiceProvider"
            ]
        }
    }
}
