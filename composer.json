{
    "name": "masterkey/repository",
    "description": "Repository Pattern for Laravel 5",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Matheus Lopes Santos",
            "email": "fale_com_lopez@hotmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=7.0.0",
        "illuminate/filesystem": "5.5.*",
        "illuminate/support": "5.5.*",
        "illuminate/container": "5.5.*",
        "illuminate/config": "5.5.*",
        "illuminate/database": "5.5.*",
        "illuminate/events": "5.5.*",
        "illuminate/pagination": "5.5.*",
        "illuminate/http": "^5.5",
        "illuminate/validation": "^5.5"
    },
    "autoload": {
        "psr-4": {
            "Masterkey\\Repository\\": "src/",
            "Masterkey\\Tests\\": "tests/"
        },
        "files": [
            "src/Exceptions/RepositoryException.php",
            "src/Exceptions/ValidationException.php"
        ]
    },
    "require-dev": {
        "phpunit/phpunit": "^6.3"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Masterkey\\Repository\\Providers\\RepositoryServiceProvider"
            ]
        }
    }
}
