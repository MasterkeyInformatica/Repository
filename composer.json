{
    "name": "masterkey/repository",
    "description": "Repository Pattern for Laravel 5",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Matheus Lopes Santos",
            "email": "fale_com_lopez@hotmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "ext-pdo": "*",
        "ext-json": "*",
        "php": "^7.4|^8.0",
        "illuminate/filesystem": "^8.0",
        "illuminate/support": "^8.0",
        "illuminate/cache": "^8.0",
        "illuminate/container": "^8.0",
        "illuminate/config": "^8.0",
        "illuminate/database": "^8.0",
        "illuminate/events": "^8.0",
        "illuminate/pagination": "^8.0",
        "illuminate/http": "^8.0",
        "illuminate/validation": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Masterkey\\Repository\\": "src/",
            "Masterkey\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.3.3"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Masterkey\\Repository\\Providers\\RepositoryServiceProvider"
            ]
        }
    }
}
